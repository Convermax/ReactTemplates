<div class="module" key="{{FieldName}}">
    <h3 class="menu-headers facet-title expanded" data-cm-role="collaps-facet">{this.DisplayName}</h3>
    <cmTemplate:SimpleFacet>
      <cmRepeater:Values>
        <div class="facet-value {this.isSelected ? 'selected':''}" key="{{Term}}">
          <div class="facet-value-chbox">{this.checkbox}</div>
          <div class="facet-value-text"><a>{{Value}} <span class="hits">({{HitCount}})</span></a><div class="clearfix"></div></div>
          <div class="clearfix"></div>
        </div>
      </cmRepeater:Values>
      <cm:ShowAll>
        <a class="expandswitcher collapsed" rt-if="!this.open">Show more <i class="icon-down-open"></i></a>
        <a class="expandswitcher expanded" rt-if="this.open">Show less <i class="icon-up-open"></i></a>
      </cm:ShowAll>
    </cmTemplate:SimpleFacet>
    <cmTemplate:ComboBoxRangedFacet>
      <div class="ranged-facet-input-wrap">
        <input type="text" data-cm-role="input-min" value={{inputMin}} placeholder="Min price" class="ranged-facet-input min" />
        <span>â€”</span>
        <input type="text" data-cm-role="input-max" value={{inputMax}} placeholder="Max price" class="ranged-facet-input max" />
        <div class="clearfix"></div>
      </div>
      <button type="button" data-cm-role="add-facet" class="btn">apply</button>
    </cmTemplate:ComboBoxRangedFacet>
  </div>
