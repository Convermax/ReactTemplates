<div class="imgblock">
  <img src="{this.post_meta__product_attributes_product_images ?
          ('//images.ladesignconceptsinc.com/'+this.post_meta__product_attributes_product_images[0].replace(/(_detail)?(\.jpe?g)$/, '_thumb$2')) :
          '//images.ladesignconceptsinc.com/image-not-available_thumb.jpg'}" alt="">
</div>
<div>
  <a class="product-link product-title">
    <rt-virtual rt-if='this.post_meta__product_attributes_pattern_name'>{this.post_meta__product_attributes_pattern_name}</rt-virtual>
    <rt-virtual rt-if='!this.post_meta__product_attributes_pattern_name && this.post_meta__product_attributes_pattern_number'>{this.post_meta__product_attributes_pattern_number}</rt-virtual>
    <span rt-if='!this.post_meta__product_attributes_pattern_name && !this.post_meta__product_attributes_pattern_number' rt-props="{ dangerouslySetInnerHTML: {__html: this.post_title}}" />
  </a>


  <div class="product-price">
    <div class="price">
      <del rt-if="this.post_meta__sale_price">
        <div class="woocommerce-Price-amount amount">
          <span class="woocommerce-Price-currencySymbol">&#36;</span> {(() => {
            if (typeof this.post_meta__regular_price !== "undefined") {
              return Number(this.post_meta__regular_price[0]).toLocaleString("en-US", {minimumFractionDigits:2});
            }
          })()}
        </div>
      </del>
      <ins rt-if="this.post_meta__regular_price">
        <div class="woocommerce-Price-amount amount">
          <span class="woocommerce-Price-currencySymbol">&#36;</span> {(() => {
            if (typeof this.post_meta__sale_price !== "undefined") {
              return Number(this.post_meta__sale_price[0]).toLocaleString("en-US", {minimumFractionDigits: 2});
            } else if (typeof this.post_meta__regular_price !== "undefined") {
              return Number(this.post_meta__regular_price[0]).toLocaleString("en-US", {minimumFractionDigits: 2});
            }})()}
        </div>
      </ins>
    </div>
  </div>
</div>
<a class="absLink" href={this.url}></a>